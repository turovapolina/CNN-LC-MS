    "idx_test_with_dubs = np.array(idx_test_with_dubs).flatten()\n",
    "X_test = np.take(final_tensor,idx_test_with_dubs,axis=0)\n",
    "y_test = np.take(Y_parts_numpy_n,idx_test_with_dubs,axis=0)"
   ]

    "idx_train_with_dubs = np.array(idx_train_with_dubs).flatten()\n",
    "X_train = np.take(final_tensor,idx_train_with_dubs,axis=0)\n",
    "y_train = np.take(Y_parts_numpy_n,idx_train_with_dubs,axis=0)"
   ]

   },
   "outputs": [],
   "source": [
    "no_dubs = np.arange(0,186,3)\n",
    "from sklearn.model_selection import train_test_split#random state 1,4,12\n",
    "idx_train, idx_test = train_test_split(no_dubs, test_size=0.3,\n",
    "                                    stratify = np.take(Y_parts_numpy_n,no_dubs),random_state=12)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 4,
   "id": "0a4c4341",
   "metadata": {
    "ExecuteTime": {
